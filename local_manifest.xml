<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="pancake" fetch="ssh://git@github.com/pe-pancake" />
  <remote name="los" fetch="https://github.com/LineageOS/" />

  <!-- PePe stuff -->
  <project
    path="device/xiaomi/davinci"
    remote="pancake"
    name="device_xiaomi_davinci"
    revision="eleven" />
  <project
    path="kernel/xiaomi/davinci"
    remote="pancake"
    name="kernel_xiaomi_davinci"
    revision="eleven" />

  <!-- Vendor from PE Official -->
  <project
    path="vendor/xiaomi/davinci"
    remote="pixel-devices-blobs"
    name="vendor_xiaomi_davinci"
    revision="eleven" />

  <!-- Hardware parts from Lineage -->
  <project
    path="hardware/xiaomi"
    name="LineageOS/android_hardware_xiaomi"
    revision="lineage-18.0"
    remote="github" />

  <!-- Proton toolchain -->
  <project
    path="prebuilts/clang/host/linux-x86/clang-proton"
    name="kdrag0n/proton-clang"
    revision="master"
    remote="github" />
</manifest>
